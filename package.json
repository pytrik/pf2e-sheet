  "scripts": {
    "dev": "node scripts/dev.mjs",
    "build": "node scripts/build.mjs",
    "build:debug": "node scripts/build.mjs --debug",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
